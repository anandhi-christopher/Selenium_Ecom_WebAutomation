<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">

	<groups>
		<run>
			<include name="regression"></include>
		</run>
	</groups>
	<listeners>
<listener class-name="tutorialNinja.Utilities.ExtentReporterNG"></listener>
<listener class-name="tutorialNinja.Utilities.TestTransformer"></listener>
</listeners>
	<test thread-count="5" name="Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="tutorialNinja.testCases.RegistrationTest"></class>
			<class name="tutorialNinja.testCases.LoginTest"/>
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->
 